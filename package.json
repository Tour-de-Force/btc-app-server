{
  "name": "btc-app-server",
  "version": "0.1.0",
  "description": "Node Server",
  "main": "es5/src/index.js",
  "dependencies": {
    "backbone": "^1.2.3",
    "backbone-couch": "^2.0.1",
    "backbone-validation": "^0.11.5",
    "body-parser": "^1.14.1",
    "config": "^1.19.0",
    "express": "^4.13.3",
    "forever": "^0.15.1",
    "js-yaml": "^3.5.2",
    "jsen": "^0.6.0",
    "jsonwebtoken": "^5.4.1",
    "morgan": "^1.6.1",
    "nano": "^6.2.0",
    "node-uuid": "^1.4.3",
    "nodemailer": "^2.1.0",
    "passport": "^0.3.2",
    "passport-jwt": "^1.2.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.0",
    "coveralls": "^2.11.6",
    "esdoc": "^0.4.4",
    "esformatter": "^0.9.0",
    "esformatter-ignore": "^0.1.3",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "fs-tools": "^0.2.11",
    "mocha": "^2.3.3",
    "nyc": "^5.6.0",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  },
  "scripts": {
    "version": "echo $npm_package_version",
    "build": "babel es6 -d es5",
    "prepublish": "npm run build",
    "format:enforce": "esformatter -i 'es6/**/*.js'",
    "format": "esformatter --diff 'es6/**/*.js'",
    "lint": "eslint 'es6/**/*.js'",
    "doc": "esdoc -c .esdoc.json",
    "start": "forever start es5/src/index.js",
    "restart": "forever restart es5/src/index.js",
    "stop": "forever stop es5/src/index.js",
    "start": "node es5/src/index.js",
    "clean": "rm -r es5/",
    "unit": "mocha './es5/test/**/*.js' --require './shim'",
    "test": "nyc npm run unit",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "shim.js",
      "es5/test/",
      "es5/src/model",
      "es5/src/register.js",
      "es5/src/util/mailer.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bikelomatic-complexity/node-server.git"
  },
  "author": "Adventure Cycling Association",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/bikelomatic-complexity/node-server/issues"
  },
  "homepage": "https://github.com/bikelomatic-complexity/node-server#readme"
}

{
  "name": "btc-app-server",
  "version": "0.2.3",
  "description": "Node Server",
  "main": "es5/src/index.js",
  "dependencies": {
    "ajv": "^3.6.2",
    "backbone": "^1.2.3",
    "backbone-pouch": "git+https://github.com/jo/backbone-pouch.git#5aef445",
    "body-parser": "^1.14.1",
    "config": "^1.19.0",
    "express": "^4.13.3",
    "express-validator": "^2.19.1",
    "js-yaml": "^3.5.2",
    "jsonwebtoken": "^5.4.1",
    "morgan": "^1.6.1",
    "nano": "^6.2.0",
    "node-uuid": "^1.4.3",
    "nodemailer": "^2.1.0",
    "passport": "^0.3.2",
    "passport-jwt": "^1.2.1",
    "pouchdb": "^5.2.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.0",
    "chai-as-promised": "^5.2.0",
    "coveralls": "^2.11.6",
    "docco": "^0.7.0",
    "esformatter": "^0.9.0",
    "esformatter-ignore": "^0.1.3",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "fs-tools": "^0.2.11",
    "mocha": "^2.3.3",
    "nyc": "^5.6.0",
    "open": "0.0.5",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  },
  "scripts": {
    "prepublish": "babel es6 -d es5",
    "format:enforce": "esformatter -i 'es6/**/*.js'",
    "format": "esformatter --diff 'es6/**/*.js'",
    "lint": "eslint 'es6/**/*.js'",
    "doc": "docco es6/src/*.js es6/src/*/*.js",
    "start": "node es5/src/index.js",
    "clean": "rm -r es5/",
    "unit": "NODE_ENV=test mocha './es5/test/**/*.js' --require './shim'",
    "test": "nyc npm run unit",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=lcov"
  },
  "nyc": {
    "exclude": [
      "shim.js",
      "es5/test/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bikelomatic-complexity/node-server.git"
  },
  "author": "Adventure Cycling Association",
